{
  "name": "autotag-action",
  "version": "1.1.3",
  "private": true,
  "description": "Automatically create a tag whenever the version changes in package.json",
  "main": "lib/main.js",
  "scripts": {
    "start": "node ./main.js",
    "build": "esbuild --bundle ./src/main.js --platform=node --outdir=dist --external:octokit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/butlerlogic/action-autotag.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "ButlerLogic",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "octokit": "^2.0.14",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "esbuild": "^0.17.14"
  }
}
